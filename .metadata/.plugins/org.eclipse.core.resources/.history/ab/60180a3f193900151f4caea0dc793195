#include "General.h"


//递归调用
int maxDepth(struct TreeNode* root) {
	if(!root) return 0;
	int leftDept,rightDept;
	leftDept = maxDepth(root->left);
	rightDept = maxDepth(root->right);

	return leftDept>rightDept?leftDept+1:rightDept+1;
}

