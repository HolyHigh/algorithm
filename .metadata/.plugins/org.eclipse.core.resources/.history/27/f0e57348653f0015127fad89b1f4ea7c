#include "General.h"


//递归查找
int binSearch(int arr[],int mete,int start,int end){
	int n = end/2;
	if(arr == NULL) return -1;
	if(arr[n] == mete) return n;
	if(arr[n] < mete) return binSearch(arr,mete,n+1,end);
	else
		return binSearch(arr,mete,0,n-1);
}
